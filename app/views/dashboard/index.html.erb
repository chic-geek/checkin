<div class="col-xs-12">
  <h1>Dashboard</h1>

  <table class="table table-striped">
    <thead>
      <tr>
        <th>#</td>
        <th>First name</td>
        <th>Last name</td>
        <th>Email</td>
      </tr>
    </thead>

    <tbody>
      <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.email %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
